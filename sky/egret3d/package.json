{
  "name": "@egret/egret3d",
  "version": "1.3.1",
  "compilerVersion": "5.3.1",
  "description": "egret 3d lib",
  "main": "bin/egret3d.js",
  "bin": {
    "egret3d": "cli/lib/index.js"
  },
  "scripts": {
    "build": "egret build && node script/build.js",
    "build-report": "egret build && node script/build.js && node script/report.js",
